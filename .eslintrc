{
  "env": {
    "browser": true,
    "es6": true,
    "mocha": true,
    "node": true
  },
  "extends": "eslint:recommended",
  "rules": {
    "array-bracket-spacing": [
      2,
      "always",
      {
        "arraysInArrays": true,
        "objectsInArrays": true,
        "singleValue": true
      }
    ],
    "block-scoped-var": 2,
    "brace-style": [
      2,
      "1tbs",
      {
        "allowSingleLine": false
      }
    ],
    "camelcase": [
      2,
      {
        "properties": "always"
      }
    ],
    "comma-dangle": [
      2,
      "never"
    ],
    "comma-spacing": [
      2,
      {
        "before": false,
        "after": true
      }
    ],
    "complexity": [
      2,
      2
    ],
    "consistent-return": 2,
    "curly": 2,
    "dot-notation": 2,
    "eol-last": 2,
    "eqeqeq": 2,
    "indent": [
      2,
      2
    ],
    "no-alert": 2,
    "no-cond-assign": [
      2,
      "always"
    ],
    "no-console": 1,
    "no-constant-condition": 2,
    "no-debugger": 2,
    "no-extra-semi": 2,
    "no-mixed-spaces-and-tabs": 2,
    "no-multi-spaces": 2,
    "no-trailing-spaces": [
      2,
      {
        "skipBlankLines": false
      }
    ],
    "no-unexpected-multiline": 2,
    "no-unused-vars": 1,
    "object-curly-spacing": [
      2,
      "always",
      {
        "arraysInObjects": true,
        "objectsInObjects": true
      }
    ],
    "quotes": [
      2,
      "single"
    ],
    "semi": [
      2,
      "always"
    ],
    "spaced-comment": 2,
    "space-in-parens": [
      2,
      "never"
    ],
    "strict": [
      2,
      "function"
    ],
    "use-isnan": 2,
    "wrap-iife": [
      2,
      "inside"
    ]
  }
}